# Example logging configuration.

. {
	level  = debug
	format = %Y-%m-%d %H:%M:%S [%thread] %LEVEL - %ns - %message
}

com.borasoftware {
	level = debug
}

balau.container {
	level = debug
}

balau.injector {
	level = debug
}

balau.network {
	level = debug
}
